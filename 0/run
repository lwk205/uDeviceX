set -ue

. ./util
. ./tsdf

s=../src
u=u/safe
c=$s/conf/lina.h

# domain types: oneA,    oneB, cyl, cyl3, narrow small, normal, big
cyl3

# [01]
nv=642 sc=0.55 radius=2.8
strt0

v=$1; shift=15; force

p_lina                   # p_[test|lina|stick]

Domain="XS=$xs YS=$ys ZS=$zs"
u.conf "$s" "$u" "$c" <<!
    $Domain
    $Force
    $Rbc
    $Param
    $Bop
    $Restart
    run
!
{ make clean && u.make -j; } > .make.log

run
