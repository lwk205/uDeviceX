u=u/x
c=conf.def.h

normal
Force="pushflow driving_force=0.1"

u.conf "$s" "$u" "$c" <<!
    $Force
    run
!
{ make clean && u.make -j; } > /dev/null
u.strtdir . $x $y $z
u.batch $x $y $z ./udx 30:00
