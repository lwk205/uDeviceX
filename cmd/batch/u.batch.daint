msg () { printf '%s\n' "$*" >&2; }
exec0() { # echo and exec
    msg cmd: "$@" #; exec "$@"
}

n=$1; shift
x=$1; shift
y=$1; shift
z=$1; shift
e=$1; shift # executable

Time='00:30:00' # default
if test ! $# -eq 0
then Time="$1"
     shift
fi

Args="-u -C gpu --time $Time $@"
exec0 srun -n $n "$Args" "$e" $x $y $z
