. u.cp.util

prog=u.test.ic0

f="$1"; shift
t="$1"; shift

Found=0
for p in "$D/ic/$f" "$D/ic/$f.ic"
do if e test -f "$p"
   then Found=1
	break
   fi
done

if test $Found -eq 0
then err "cannot find ic file '$f=$p'"
fi

e cp '2>/dev/null' "$p" "$t"
if test $? -ne 0
then err "fail to copy to '$t'"
fi
