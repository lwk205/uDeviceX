. u.utils

prog=u.argp

usg () {
     msg "$prog"' conf.def.h A=1 B=2 ... > conf.h'
     msg 'conf.h generator'
     exit
}

if test $# -ne 0 && test "$1" = -h; then usg; fi

if test $# -eq 0
then err "$prog: needs an argument"
fi

f="$1"; shift

if test ! -f "$f"
then err "$prog: not a file '$f'"
fi

exec u.argp0 "$f" "$@"
