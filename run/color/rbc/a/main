u=u/x
Domain="XS=$XS YS=$YS ZS=$ZS"
u.conf $s $u $c <<!!!
   tend=4.01 part_freq=800
   $Domain
   recolor=100
   aij_solv=2 aij_rbc=2 aij_wall=4
   kBT=0.000444302
   driving_force=0.6
   pushflow doublepoiseuille
   rbcs
   field_dumps part_dumps
   field_freq=8000
   run
!!!
(make clean && u.make -j) > /dev/null
sh runfile
