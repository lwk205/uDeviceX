#### spheres in double poiseuille, contact
# nTEST: multisolid.t2
rm -rf bop h5 diag.txt solid_diag*.txt
echo -e "4.1 21.5 8\n4.1 10.5 8\n11.9 16 8" > rigs-ic.txt
cp data/rig/sphere.ply rig.ply
:
u.conf conf/double.poiseuille.h <<!!!
  solids sbounce_back tend=0.5
  wall_creation=0 contactforces=true
  run
!!!
{ make clean && u.make -j ; } > /dev/null
u.run ./udx '
glb = {
    dt = 5e-4
    L = [16, 32, 16]
}
bforce = {
    type = "double_poiseuille";
    a    = 6.666667;
};
dump = {
    field = true;
    parts = true;
    freq_field = 100;
    freq_parts = 100;
};'
cat solid_diag_0000.txt | awk '{print $2, $5, $8}' | uscale 5 > sdiag.out.txt
