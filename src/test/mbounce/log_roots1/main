#### Double poiseuille non pinned sphere
# snTEST: mbounce.log_roots.t1
rm -rf h5 bop diag.txt solid_diag.txt
echo -e "8 16 8" > rigs-ic.txt
cp data/rig/sphere.ply rig.ply
:
u.conf conf/default.h <<!!!
   kBT=1.0 solids sbounce_back
   part_freq=100
   MESHBB_LOG_ROOTS
   field_dumps part_dumps
   field_freq=100
   run
!!!
{ make clean && u.make -j ; } > /dev/null
u.run ./udx '
glb = {
    L = [16, 32, 16]
}
bforce = {
    type = "constant"
    f = [0.0333333, 0.0, 0.0]
}' 2>/dev/null
:
awk '$NF == ":log_root:" { $NF = ""; print}' .000 > cubic.txt
