#### Double poiseuille non pinned sphere
# snTEST: mbounce.log_roots.t1
cd test/mbounce/log_roots1
S=`u.cp.s`
echo -e "8 16 8" > rigs-ic.txt
u.cp.rig sphere.ply rig.ply
:
u.conf "$S" u/x "$S/conf/default.h" <<!
   kBT=1.0
   part_freq=100
   MESHBB_LOG_ROOTS
   field_dumps part_dumps
   field_freq=100
   run
!
u.make -j
u.run ./udx conf.cfg
awk '$NF == ":log_root:" { $NF = ""; print}' .000 > cubic.txt
