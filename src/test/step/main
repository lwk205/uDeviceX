#### Double poiseuille
# scTEST: step.0
set -e
rm -rf bop h5 diag.txt
:
u=u/safe
s=`pwd`
u.conf "$s" $u conf/double.poiseuille.h <<!!!
   numberdensity=1
   tend=1e4
   run
!!!
(make clean && u.make -j) > /dev/null
u.run ./udx '
glb = {
    dt = 1e3
    L = [16, 32, 16]
}
dump = {
    field = true;
    parts = true;
    freq_field = 4000;
    freq_parts = 4000;
};'
u.avgh5 0 u 1 h5/flowfields-0001.h5 | uscale 0.1 > h5.out.txt
