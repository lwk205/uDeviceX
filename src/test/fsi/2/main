#### with fsi and mpi
# nTEST: fsi.t3
cd test/fsi/2
S=`u.cp.s`
u.cp.rbc rbc/498.off rbc.off
 x=5 y=17 z=8; echo 1 0 0 $x  0 1 0 $y  0 0 1 $z  0 0 0 1 >  rbcs-ic.txt
:
u.conf "$S" u/x "$S/conf/default.h" <<!
   rbcs
   pushrbc=false fsiforces=true
   run
!
u.make -j
u.run 1 2 1 ./udx conf.cfg
ply2punto r/00001.ply > ply.out.txt
