#### no fsi
# nTEST: fsi.t1
cd test/fsi/0
S=`u.cp.s`
rm -rf bop diag.txt h5 ply rbc.off
u.cp.rbc rbc/498.off rbc.off
 x=5 y=17 z=8; echo 1 0 0 $x  0 1 0 $y  0 0 1 $z  0 0 0 1 >  rbcs-ic.txt
:
u.conf "$S" u/x "$S/conf/default.h" <<!
   rbcs
   pushrbc=false fsiforces=false
   run
!
{ make clean &&  u.make -j ; } > /dev/null
u.run ./udx conf.cfg
ply2punto r/00001.ply | uscale 100 > ply.out.txt
