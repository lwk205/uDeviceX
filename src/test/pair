# TEST: pair.0
s=. b=.
u.conf0 u/pair
{ u.make clean && u.make -j;     } > /dev/null
u.run $b/udx < $s/data/pair/2 > pair.out.txt

# TEST: pair.1
s=. b=. c=conf/default.h
u.conf $s u/pair $c <<!
multi_solvent=false
run
!
{ u.make clean && u.make -j;     } > /dev/null
u.run $b/udx < $s/data/pair/rnd > pair.out.txt
