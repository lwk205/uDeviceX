#### compute area and volume
# TEST: rbc.u.area_volume.dev.0
S=`u.cp.s` U=u/rbc/area_volume C=conf/lina.h
nv=1986
cp $S/data/cells/rbc/$nv.off rbc.off
cp $S/data/ic/ident          rbcs-ic.txt
u.conf $S $U $C <<!
KL_SYNC RBCnv=$nv RBC_RND=false
run
!
{ u.make clean && u.make -j; } > /dev/null
u.run ./udx '
glb = {L = [20, 20, 20]}
'> av.out.txt
