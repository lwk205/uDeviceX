#### check forces on a cell
# nTEST: rbc.u.simple
cd test/rbc/u/simple
S=`u.cp.s`
nv=642
u.cp.rbc rbc/$nv.off rbc.off
u.cp.ic  two         rbcs-ic.txt
area=`off.area rbc.off`
volume=`off.volume rbc.off`
u.conf0 u/rbc/force
u.make -j
u.run ./udx conf.cfg "
rbc = {
    active = true
    totArea = $area
    totVolume = $volume
} " | uscale 0.1 >  forces.out.txt
