#### check forces on a cell
# nTEST: rbc.u.simple
S=`u.cp.s` U=u/rbc/force C="$S/conf/default.h"
:
nv=642
u.conf $S $U $C <<!
       RBCnv=$nv kBT=0 fsiforces=false
       run
!
{ u.make -j;     } > /dev/null
u.run ./udx "
time = {
    type = \"const\"
    dt = 5e-4
}
glb = {
    L = [16, 16, 16]
}
rbc = {
    cell = \"data/cells/rbc/$nv.off\"
    ic = \"data/ic/two\"
    @include \"rbc.test.cfg\"
}" | uscale 0.1 >  forces.out.txt
