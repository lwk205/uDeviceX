#### solvent only restart
# nTEST: rstrt.t1
rm -rf bop h5 diag.txt bin.1 bin.2
:
u.conf conf/double.poiseuille.h <<!!!
   tend=0.01
   wall_creation=0
   BASE_STRT_DUMP=\"bin.1/strt\"
   strt_dumps=true
   strt_freq=10000
   RESTART=false
   run
   tend=2.01
   RESTART=true
   BASE_STRT_READ=\"bin.1/strt\"
   BASE_STRT_DUMP=\"bin.2/strt\"
   run
!!!
:
u.strtdir bin.1
u.strtdir bin.2
(make clean && u.make -j) > /dev/null
u.run ./bin.1/udx '
glb = {
    L = [16, 32, 16]
}
dump = {
    field = true;
    parts = true;
    freq_field = 4000;
    freq_parts = 4000;
};'
u.run ./bin.2/udx '
glb = {
    L = [16, 32, 16]
}
bforce = {
    type = "double_poiseuille";
    a    = 6.666667;
};
dump = {
    field = true;
    parts = true;
    freq_field = 4000;
    freq_parts = 4000;
};'
u.avgh5 0 u 1 h5/flowfields-0000.h5 | uscale 0.1 > h5.out.txt
