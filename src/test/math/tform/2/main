# TEST: math.tform.2
set -e -u
S=`u.cp.s` U=u/math/tform C="$S/conf/default.h"
u.conf "$S" "$U" "$C" <<!
run
!
{ u.make clean && u.make -j; } > .make.log
t1="0 0 0 1 2 3    1 2 3 10 20 30"
t2="1 2 3 0 0 0    10 20 30 1 2 3"
u.run >tform.txt ./udx -- a=\"-c $t1    $t2\"  <<!
1 2 3
10 20 30
0.5 1 1.5
!
mv tform.txt tform.out.txt
