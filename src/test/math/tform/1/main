# TEST: math.tform.1
set -e -u
S=`u.cp.s` U=u/math/tform C="$S/conf/default.h"
u.conf "$S" "$U" "$C" <<!
run
!
{ u.make clean && u.make -j; } > .make.log
u.run >tform.txt ./udx -- '
a="0 0 0 1 2 3    1 2 3 10 20 30"
glb = {
    L = [16, 16, 16]
}'  <<!
1 2 3
10 20 30
0.5 1 1.5
!
mv tform.txt tform.out.txt
