#### two consequent runs (no connections)
# nTEST: strt.t0
rm -rf bop h5 diag.txt rbc.off
:
export PATH=../tools:$PATH
(cd ../cmd; make ) > /dev/null
cp data/cells/rbc.498.off  rbc.off
:
u.conf conf/double.poiseuille.h <<!!!
   tend=2.01 part_freq=4000
   FORCE_DOUBLE_POISEUILLE
   field_dumps part_dumps
   field_freq=4000
   run
   run
!!!
(make clean && u.make -j) > /dev/null
sh runfile
avg_h52.m h5/flowfields-0001.h5 | uscale 0.1 > h5.out.txt

#### two consequent runs
# nTEST: strt.t1
rm -rf bop h5 diag.txt rbc.off
:
export PATH=../tools:$PATH
(cd ../cmd; make ) > /dev/null
cp data/cells/rbc.498.off  rbc.off
:
u.conf conf/double.poiseuille.h <<!!!
    tend=2.01 part_freq=4000
    FORCE_DOUBLE_POISEUILLE
    field_dumps part_dumps
    field_freq=4000
    run
    FORCE_NONE
    run
!!!
(make clean && u.make -j) > /dev/null
sh runfile
avg_h52.m h5/flowfields-0001.h5 | uscale 0.1 > h5.out.txt

