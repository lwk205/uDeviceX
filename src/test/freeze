#### freeze after high temp
# cTEST: freeze.t0
export PATH=../tools:$PATH
rm -rf bop h5 diag.txt
c=conf/zurich.h
cp data/sdf/yplates1/yplates.dat sdf.dat
u.conf $c <<!!!
   numberdensity=3
   walls wall_creation=10 shear_y
   S_LEVEL=0 kBT=1 adpd_b=12.5 gdpd_b=5.61125
   XS=12 YS=32 ZS=12
   dt=0.05 tend=41 part_freq=800 field_freq=800
   field_dumps part_dumps
   run
!!!
(make clean && u.make -j) > make.log
sh runfile
:
avg_h52.m h5/flowfields-0001.h5 | uscale 0.1 > h5.out.txt

