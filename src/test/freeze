#### freeze after high temp
# cTEST: freeze.t0
export PATH=../tools:$PATH
rm -rf bop h5 diag.txt
c=conf/zurich.h
cp data/sdf/yplates1/yplates.dat sdf.dat
Domain="XS=12 YS=32 ZS=12"
Walls="walls wall_creation=10 shear_y gamma_dot=0.36"
A="adpd_b=12.5 adpd_r=12.5 adpd_br=40.0"
Gamma="gdpd_b=5.61125 gdpd_r=5.61125 gdpd_br=5.61125"
u.conf $c <<!!!
   numberdensity=3
   $A $Gamma kBT=1 S_LEVEL=0
   $Walls
   $Domain
   dt=0.05 tend=400.01 part_freq=8000 field_freq=8000
   field_dumps part_dumps
   run
!!!
(make clean && u.make -j) > make.log
sh runfile
:
avg_h5.m h5/flowfields-0001.h5 | uscale 0.1 > h5.out.txt

