#### "Pachinko"
# nTEST: pachinko
rm -rf bop h5 diag.txt rbc.off
:
set -eu
S=`pwd`
cp "$S"/data/sdf/pachinko/sdf.dat sdf.dat
:
Force="bforce={type=\"constant\"; f=[10.,0.,0.];};"
u.conf conf/test.h <<!!!
   XS=32 YS=56 ZS=60 
   tend=1.0 part_freq=300 walls wall_creation=100
   field_dumps part_dumps field_freq=300
   run
!!!
{ make clean && u.make -j; } > /dev/null
u.run ./udx $Force
u.avgh5 1 u 2 h5/flowfields-0003.h5 | uscale 0.1 > h5.out.txt
