rm -rf bop h5 diag.txt rbc.off
:
set -eu
S=`u.cp.s`
u.cp.sdf pachinko/sdf.dat sdf.dat
:
u.conf conf/test.h <<!!!
   walls
   run
!!!
{ make clean && u.make -j; } > /dev/null
u.run ./udx '
time = {
    type = "const"
    dt = 5e-4
    wall = 5e-2
    end = 1.0
}
glb = {
    L = [32, 56, 60]
}
bforce = {
    type = "constant"
    f    = [10.,0.,0.]
}
dump = {
    field = true
    parts = true
    freq_field = 0.1500
    freq_parts = 0.1500
}'
u.avgh5 1 u 2 h5/flowfields-0002.h5 | uscale 0.1 > h5.out.txt
