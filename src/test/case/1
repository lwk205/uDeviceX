# nTEST: case.1
set -eu
:
D=case0 S=`pwd` U=u/x C="$S/conf/test.h"
rm -rf "$D"
:
export S
W="$S/data/sdf/wall1/wall.dat"
u.case -d "$D" -f "$W" sdf.dat -a "u.run ./udx" \
$U $C <<!
   XS=12 YS=12 ZS=12
   tend=1.0 part_freq=300 walls wall_creation=1
   field_dumps part_dumps field_freq=300
   WVEL_PAR_Z
   run
!
u.avgh5 1 u 2 case0/h5/flowfields-0003.h5 | \
    uscale 0.1 > h5.out.txt
