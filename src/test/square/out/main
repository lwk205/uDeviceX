## using gitlab:ugeom
# TEST: square.out
S=`u.cp.s` U=u/x C="$S/conf/default.h"
XS=8 YS=8 ZS=8 a=7
ugeom.square $XS $YS $ZS $a sdf.dat
u.conf "$S" "$U" "$C" <<!
  KL_SYNC
  walls
  run
!
u.make -j
u.run ./udx "
time = {
    type = \"const\"
    dt = 1e-4
    wall = 1e-4
    end = 1e-4
}
glb = {
    L = [$XS, $YS, $ZS]
}
dump = {
    field = true
    freq_field = 0.2000
}"
cp h5/wall.xmf wall.out.xmf
