ugeom.square $XS $YS $ZS $a sdf.dat
u.conf "$S" "$U" <<!
  KL_SYNC
  run
!
u.make -j
u.run ./udx "
time = {
    type = \"const\"
    dt = 1e-4
    wall = 1e-4
    end = 1e-4
}
wall = {
    active = true
}
glb = {
    L = [$XS, $YS, $ZS]
}
dump = {
    field = true
    freq_field = 0.2000
}"
cp h5/wall.xmf wall.out.xmf
