## using gitlab:ugeom
# TEST: square.in
set -eu
S=`pwd` U=u/x C="$S/conf/test.h"
XS=8 YS=8 ZS=8 a=3
ugeom.square -i $XS $YS $ZS $a sdf.dat
u.conf "$S" "$U" "$C" <<!
  KL_SYNC
  dt=1e-4 tend=1e-4 field_dumps walls wall_creation=1
  XS=$XS YS=$YS ZS=$ZS
  run
!
{ u.make clean && u.make -j; } > .make.log
u.run ./udx
cp h5/wall.xmf wall.out.xmf
