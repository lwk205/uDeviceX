#### random particles, some are outside
# TEST: clist.t2
set -eu
S=`u.cp.s`
X=4 Y=6 Z=8 N=64
u.conf . u/clist conf/default.h <<!!!
       run
!!!
{ u.make -j;     } > /dev/null
u.pre.gen_random 20 $Y $Z $N > particles.out 
u.run ./udx "
glb = {
    L = [$X, $Y, $Z];
};" < particles.out > clist.out.txt
