#### KL_NONE
# nTEST: long.t1
rm -rf bop h5 diag.txt rbc.off
:
export PATH=../tools:$PATH
(cd ../cmd; make ) > /dev/null
:
u.conf conf/long.h <<!!!
   KL_NONE
   run
!!!
(make clean && u.make -j) > /dev/null 2>&1
sh runfile
cat diag.txt | awk 'NR>1 {print $1}' | uscale 1 > sdiag.out.txt

#### FORCE0
# nTEST: long.t2
rm -rf bop h5 diag.txt rbc.off
:
export PATH=../tools:$PATH
(cd ../cmd; make ) > /dev/null
:
u.conf conf/long.h <<!!!
   FORCE0
   run
!!!
(make clean && u.make -j) > /dev/null 2>&1
sh runfile
cat diag.txt | awk 'NR>1 {print $1, $2, $3}' | uscale 1 > sdiag.out.txt

