#### Couette pinned cylinder
# nTEST: solid.t3
rm -rf h5 bop diag.txt solid_diag.txt
u.cp.sdf yplates1/yplates.dat sdf.dat
echo -e "16 16 8" > rigs-ic.txt
cp data/rig/cyl.ply rig.ply
:
u.conf conf/test.h <<!!!
   walls
   solids spdir=2 sbounce_back
   run
!!!
{ make clean && u.make -j ; } > /dev/null
u.run ./udx '
time = {
    type = "const"
    dt = 1e-3
    wall = 1e-1
    end = 2.01
}
glb = {
    L = [32, 32, 16]
}
wvel = {
    type = "shear";
    gdot = 5.0;
    vdir = 0;
    gdir = 1;
};
dump = {
    field = true;
    parts = true;
    freq_field = 0.5000
    freq_parts = 1000;
};'
u.avgh5 1 u 2 h5/flowfields-0001.h5 | sed -n '4,29p' | uscale 0.5 > h5.out.txt
