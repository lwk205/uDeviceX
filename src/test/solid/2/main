u.cp.sdf yplates1/yplates.dat sdf.dat
echo -e "16 16 8" > rigs-ic.txt
cp data/rig/cyl.ply rig.ply
:
u.conf conf/test.h <<!!!
   tend=2.01 walls wall_creation=100 ZS=16
   solids spdir=2 sbounce_back dt=1e-3
   run
!!!
{ make clean && u.make -j ; } > /dev/null
u.run ./udx '
wvel = {
    type = "shear";
    gdot = 5.0;
    vdir = 0;
    gdir = 1;
};
dump = {
    field = true;
    parts = true;
    freq_field = 500;
    freq_parts = 1000;
};'
u.avgh5 1 u 2 h5/flowfields-0001.h5 | sed -n '4,29p' | uscale 0.5 > h5.out.txt
