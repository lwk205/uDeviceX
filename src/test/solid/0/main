:
u.conf conf/test.h <<!!!
   walls
   solids sbounce_back pin_com
   run
!!!
{ make clean && u.make -j ; } > /dev/null
u.run ./udx '
time = {
    type = "const"
    dt = 1e-3
    end = 2.01
}
glb = {
    L = [32, 32, 32]
}
wvel = {
    type = "shear";
    gdot = 5.0;
    vdir = 0;
    gdir = 2;
};
dump = {
    field = true;
    parts = true;
    freq_field = 1.0000
    freq_parts = 1000;
};'
u.avgh5 1 u 2 h5/flowfields-0001.h5 | sed -n '4,29p' | uscale 0.25 > h5.out.txt
