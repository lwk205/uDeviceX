#### two solids around cylinder with one solid removed by the wall on 2 ranks
# nTEST: solid.t5
cd test/solid/4
S=`u.cp.s`
u.cp.sdf cyl1/cyl.dat sdf.dat
u.cp.rig sphere_R1.ply rig.ply
x=0.75 y=3 z=9; echo 1 0 0 $x  0 1 0 $y  0 0 1 $z  0 0 0 1 >  rigs-ic.txt
x=8    y=8 z=8; echo 1 0 0 $x  0 1 0 $y  0 0 1 $z  0 0 0 1 >> rigs-ic.txt
:
u.run 2 1 1 udx conf.cfg
cat diag/rig.0000.txt | awk '{print $2, $5, $8}' | uscale 5 > sdiag.out.txt
