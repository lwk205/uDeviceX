#### two solids around cylinder with one solid removed by the wall on 2 ranks
# nTEST: solid.t5
rm -rf h5 bop diag.txt solid_diag.txt
u.cp.sdf cyl1/cyl.dat sdf.dat
cp data/rig/sphere_R1.ply rig.ply
x=0.75 y=3 z=9; echo $x $y $z >  rigs-ic.txt
x=8    y=8 z=8; echo $x $y $z >> rigs-ic.txt
:
u.conf conf/default.h <<!!!
   solids walls kBT=1e-7
   run
!!!
{ make clean && u.make -j ; } > /dev/null
u.run 2 1 1 ./udx '
time = {
    type = "const"
    dt = 5e-4
    wall = 5e-2
    end = 1.0
}
glb = {
    L = [8, 16, 16]
}
bforce = {
    type = "constant"
    f    = [1.6666667,0.,0.]
}
dump = {
    field = true
    parts = true
    freq_field = 0.2500
    freq_parts = 0.2500
}'
cat solid_diag_0000.txt | awk '{print $2, $5, $8}' | uscale 5 > sdiag.out.txt
