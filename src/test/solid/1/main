#### Double poiseuille non pinned sphere
# nTEST: solid.t2
cd test/solid/1
S=`u.cp.s`
echo -e "8 16 8" > rigs-ic.txt
u.cp.rig sphere.ply rig.ply
:
u.conf "$S" u/x "$S/conf/default.h" <<!
   solids sbounce_back
   run
!
u.make -j
u.run ./udx conf.cfg
cat solid_diag_0000.txt | awk '{print $2, $5, $8}' | uscale 5 > sdiag.out.txt
