#### contact force: RBC and sphere in double Poiseuille
# nTEST: mix.contact.t1
rm -rf diag.txt h5 bop ply
 x=5 y=17 z=8; echo 1 0 0 $x  0 1 0 $y  0 0 1 $z  0 0 0 1 > rbcs-ic.txt
x=11 y=15 z=8; echo $x $y $z                              > rigs-ic.txt
cp data/rig/sphere_R2.ply rig.ply
u.cp.rbc rbc/498.off rbc.off
:
S=`u.cp.s`
u.conf "$S" u/x "$S/conf/default.h" <<!
   rbcs
   solids sbounce_back
   pushrbc
   contactforces
   run
!
{ make clean &&  u.make -j ; } > /dev/null
u.run ./udx  conf.cfg
ply2punto r/00001.ply | uscale 1 > ply.out.txt
