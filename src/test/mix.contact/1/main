#### no contact force: RBC and sphere in double Poiseuille
# nTEST: mix.no.contact.t1
rm -rf diag.txt h5 bop ply
 x=5 y=17 z=8; echo 1 0 0 $x  0 1 0 $y  0 0 1 $z  0 0 0 1 > rbcs-ic.txt
x=11 y=15 z=8; echo $x $y $z                              > rigs-ic.txt
cp data/rig/sphere_R2.ply rig.ply
u.cp.rbc rbc/498.off rbc.off
:
u.conf conf/double.poiseuille.h <<!!!
   rbcs
   solids sbounce_back
   pushrbc
   run
!!!
{ make clean &&  u.make -j ; } > /dev/null
u.run ./udx '
time = {
    type = "const"
    dt = 5e-4
    end = 2.0
}
glb = {
    L = [16, 32, 16]
}
rbc = {
    @include "rbc.test.cfg"
}
bforce = {
    type = "double_poiseuille";
    a = 6.666667;
};
dump = {
    field = true;
    parts = true;
    freq_field = 0.7500
    freq_parts = 0.7500
};'
ply2punto r/00002.ply | uscale 1 > ply.out.txt
