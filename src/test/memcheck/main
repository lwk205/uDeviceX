    L = [16, 32, 16]
}
bforce = {
    type = "double_poiseuille";
    a    = 6.666667;
};
dump = {
    field = true;
    parts = true;
    freq_field = 0.0500
    freq_parts = 0.0500
};' | awk '/========= ERROR SUMMARY:/'         > mem.out.txt
