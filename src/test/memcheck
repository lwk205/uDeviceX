#### spheres in double poiseuille, contact
# TEST: memcheck.t0
export PATH=../tools:$PATH
(cd ../cmd; make ) > /dev/null
rm -rf bop h5 diag.txt solid_diag*.txt
echo -e "4.1 21.5 8\n4.1 10.5 8\n11.9 16 8" > rigs-ic.txt
cp data/rig/sphere.ply mesh_solid.ply
:
u.conf . u/dang conf/double.poiseuille.h <<!!!
  KL_SYNC
  solids tend=0.005 part_freq=100
  FORCE_DOUBLE_POISEUILLE field_dumps part_dumps
  wall_creation=0 contactforces=true field_freq=100
  run
!!!
(make clean && u.make -j && make -C ../tools) > /dev/null
MEM= sh runfile | awk '/========= ERROR SUMMARY:/'         > mem.out.txt
