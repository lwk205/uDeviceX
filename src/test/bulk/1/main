#### random particles; density 10
# nTEST: bulk.t1
X=4; Y=4; Z=4; N=640
u.conf . u/bulk conf/default.h <<!!!
       kBT=0 multi_solvent=false    
       run
!!!
{ u.make -j;     } > /dev/null
u.pre.gen_random $X $Y $Z $N > pp.txt
u.run ./udx "
in = \"pp.txt\"; 
out = \"ff.txt\"
glb = {
    L = [$X, $Y, $Z];
};"
uscale 100 ff.txt | sort > ff.out.txt

