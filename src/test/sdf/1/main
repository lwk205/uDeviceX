# TEST: sdf.1
set -eu
S=`u.cp.s` U=u/sdf C=conf/default.h
u.conf "$S" "$U" "$C" <<!
run
!
(u.make clean && u.make -j) > /dev/null
u.cp.sdf cyl1/cyl.dat  sdf.dat
mkdir -p h5
u.run ./udx '
pos = [0.0, 0.0, 0.0]
glb = {
    L = [16, 16, 16]
}
dump = {
    field = true;
    freq_field = 2000;
};'
u.wall.dump h5/wall.h5     > wall.out.txt
