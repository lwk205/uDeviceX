:
u.conf conf/default.h <<!
   OUTFLOW_CIRCLE OUTFLOW_CIRCLE_R=8 kBT=0
   run
!
u.make -j
u.run 2 2 1 ./udx test/openbc/out.cfg '
time = {
    type = "const"
    dt = 5e-4
    end = 2.01
}
glb = {
    L = [8, 8, 16]
}
dump = {
    field = true
    parts = true
    freq_field = 2.0000
    freq_parts = 2.0000
}'
u.avgh5 0 u 0 h5/0001.h5 | uscale 10 > h5.out.txt
