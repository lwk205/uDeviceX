#### rest with deleting cylinder and cylinder inflow 
# nTEST: inout.color.t1
u.conf conf/default.h <<!!!
   tend=2.01
   OUTFLOW_CIRCLE OUTFLOW_CIRCLE_R=8
   multi_solvent
   run
!!!
(make clean && u.make -j) > /dev/null
u.run ./udx test/openbc/inout.cfg '
glb = {
    dt = 5e-4
    L = [16, 16, 16]
}
dump = {
    field = true;
    parts = true;
    freq_field = 4000;
    freq_parts = 4000;
};'
u.avgh5 0 u 0 h5/flowfields-0001.h5 | uscale 10 > h5.out.txt
