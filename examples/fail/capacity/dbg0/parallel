#### Double poiseuille
# sTEST: fail.capacity.parallel
:
set -eu
S=`pwd` U=u/x C=conf/double.poiseuille.h
Force="bforce={type=\"constant\"; f = [1e5, 1e5, 0.0];};"
u.conf $S $U $C <<!
   tend=2.01
   run
!
{ make clean && u.make -j; } > .make.log
u.run ./udx $Force
u.avgh5 0 u 1 h5/flowfields-0001.h5 | uscale 0.1 > h5.out.txt
